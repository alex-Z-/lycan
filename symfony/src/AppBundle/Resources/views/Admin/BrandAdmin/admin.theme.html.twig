{% block sonata_admin_orm_one_to_many_widget %}
    {% if sonata_admin.field_description.name  == "channels" %}
        {% if not sonata_admin.admin.hasNoProviders %}
            {% include 'SonataDoctrineORMAdminBundle:CRUD:edit_orm_one_to_many.html.twig' %}
        {% endif %}
    {% else %}
        {% include 'SonataDoctrineORMAdminBundle:CRUD:edit_orm_one_to_many.html.twig' %}
    {% endif %}

    {% if sonata_admin.admin.hasNoProviders %}
        {% if  sonata_admin.field_description.name is defined and  sonata_admin.field_description.name  == "channels"  %}
            <ul class="nav navbar-nav">
                {{ knp_menu_render('AppBundle:Menu\ProvidersBuilder:createMenu', {'currentClass': 'active', 'template': 'AppBundle:Menu:knp_menu.html.twig'}) }}
            </ul>
        {% endif %}
    {% endif %}



{% endblock %}