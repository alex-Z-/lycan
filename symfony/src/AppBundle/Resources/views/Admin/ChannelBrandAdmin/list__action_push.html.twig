{% if object.pushInProgress and object.lastActiveBatch %}
    <a class="btn btn-sm btn btn-sm btn-default"  href="{{ url('admin_providers_core_batchexecutions_event_list', { 'id' : object.lastActiveBatch.id }) }}"><i class="fa fa-book fa-fw"></i> View Job</a>
    <a style='white-space: nowrap;' data-container='body' data-toggle="tooltip" data-placement="right" title="Sync Currently Running" class="btn btn-sm btn btn-sm btn-danger pull_link"  href="{{ admin.generateObjectUrl('pushStop', object) }}"><i class="fa fa-refresh fa-spin fa-fw"></i> Stop Push</a>
{% else %}
    <a class="btn btn-sm btn btn-sm btn-info pull_link" href="{{ admin.generateObjectUrl('push', object) }}"><i class="fa fa-upload" aria-hidden="true"></i> Push</a>
{% endif %}